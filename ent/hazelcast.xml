<hazelcast xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://www.hazelcast.com/schema/config
                               http://www.hazelcast.com/schema/config/hazelcast-config-3.8.xsd"
           xmlns="http://www.hazelcast.com/schema/config">

    <properties>
        <property name="hazelcast.socket.bind.any">false</property>

    </properties>
    <group>
        <name>clusterB</name>
        <password>clusterB-pass</password>
    </group>

    <management-center enabled="false">http://10.0.1.4:8080/mancenter/</management-center>

    <network>
	<public-address>10.0.1.17:5701</public-address>
	</network>
	<license-key></license-key>
</hazelcast>

